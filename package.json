  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "3.2.0",
    "@types/chrome": "0.0.190",
    "@types/node": "17.0.43",
    "@types/react": "18.0.12",
    "@types/react-dom": "18.0.5",
    "prettier": "2.7.0",
    "typescript": "4.7.4"
  },
